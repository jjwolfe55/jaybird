INSERT INTO mri_preventing_fielding (
player_id,
player_name,
team_id,
season_id,
age,
throw,
d_pos,
mri_fielding_rc,
mri_fielding_raa,
mri_fielding_waa,
mri_fielding_war
)



SELECT 
a.player_id AS player_id,
a.player_name AS player_name,
a.team_id AS team_id,
a.season_id AS season_id,
a.age,
a.throw,
a.d_pos,
b.mri_fielding_rc
c.mri_fielding_raa,
c.mri_fielding_waa,
d.mri_fielding_war
FROM standard_fielding AS a
INNER JOIN mri_preventing_fielding_runs AS b
ON a.player_id = b.player_id AND a.team_id = b.team_id AND a.season_id = b.season_id
INNER JOIN mri_preventing_fielding_wins AS c
ON a.player_id = c.player_id AND a.team_id = c.team_id AND a.season_id = c.season_id
INNER JOIN mri_preventing_fielding_war AS d
ON a.player_id = d.player_id AND a.team_id = d.team_id AND d.season_id = d.season_id
