)
, rpt_seventeen AS (
SELECT b.age, COUNT(b.player_id), (SUM(b.<xx>) - SUM(a.<xx>))/ (SUM(a.<xx>)) AS <xx>_delta
FROM s_sixteen AS a
INNER JOIN s_seventeen AS b
