FROM rpt_twenty;

WITH player_pool AS (
SELECT player_id 
FROM career_batting
WHERE seasons > 9 AND pa > 2999
)
,s_zero AS (
SELECT a.player_id, a.age, a.<xx>
FROM season_c_baserunning AS a
INNER JOIN season_batting AS b
ON a.player_id = b.player_id AND a.season_id = b.season_id
INNER JOIN player_pool AS c
ON a.player_id = c.player_id
WHERE b.pa > 300 AND a.age = 20 
)
,s_one AS (
SELECT a.player_id, a.age, a.<xx>
FROM season_c_baserunning AS a
INNER JOIN season_batting AS b
ON a.player_id = b.player_id AND a.season_id = b.season_id
INNER JOIN player_pool AS c
ON a.player_id = c.player_id
WHERE b.pa > 300 AND a.age = 21 
)
,s_two AS (
SELECT a.player_id, a.age, a.<xx>
FROM season_c_baserunning AS a
INNER JOIN season_batting AS b
ON a.player_id = b.player_id AND a.season_id = b.season_id
INNER JOIN player_pool AS c
ON a.player_id = c.player_id
WHERE b.pa > 300 AND  a.age = 22
)
,s_t<xx>ee AS (
SELECT a.player_id, a.age, a.<xx>
FROM season_c_baserunning AS a
INNER JOIN season_batting AS b
ON a.player_id = b.player_id AND a.season_id = b.season_id
