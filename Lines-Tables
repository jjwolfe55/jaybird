);

CREATE TABLE lines_rosters (
player_id VARCHAR(9),
last_name VARCHAR(20),
first_name VARCHAR(20),
bat CHAR(1),
throw CHAR(1),
team_id VARCHAR(3),
g SMALLINT,
g_p SMALLINT,
g_sp SMALLINT,
g_rp SMALLINT,
g_c SMALLINT,
g_1b SMALLINT,
g_2b SMALLINT,
g_3b SMALLINT,
g_ss SMALLINT,
g_lf SMALLINT,
g_cf SMALLINT,
g_rf SMALLINT,
g_of SMALLINT,
g_dh SMALLINT,
g_ph SMALLINT,
g_pr SMALLINT,
first_g INT,
last_g INT,
PRIMARY KEY (player_id,team_id)
);

CREATE TABLE lines_batting (
game_id VARCHAR(15),
player_id VARCHAR(9),
team_id VARCHAR(3),
b_lineup TINYINT,
b_sequence TINYINT,
integrity CHAR(8),
b_pa TINYINT,
b_ab TINYINT,
b_r TINYINT,
b_h TINYINT,
b_d TINYINT,
b_t TINYINT,
b_hr TINYINT,
b_rbi TINYINT,
b_sh TINYINT,
b_sf TINYINT,
b_hbp TINYINT,
b_bb TINYINT,
b_ibb TINYINT,
b_so TINYINT,
b_sb TINYINT,
b_cs TINYINT,
b_gdp TINYINT,
b_ci TINYINT,
b_roe TINYINT,
b_dh TINYINT,
b_ph TINYINT,
b_pr TINYINT,
game_date INT,
game_count TINYINT,
ballpark VARCHAR(7),
opponent VARCHAR(3),
win_loss CHAR(2),
game_type VARCHAR(15),
box_score CHAR(2),
PRIMARY KEY (game_id,player_id,team_id)
);

CREATE TABLE lines_pitching (
game_id VARCHAR(15),
player_id VARCHAR(9),
team_id VARCHAR(3),
p_sequence TINYINT,
integrity CHAR(8),
p_ipouts TINYINT,
p_noout TINYINT,
p_bf TINYINT,
p_h TINYINT,
p_d TINYINT,
p_t TINYINT,
p_hr TINYINT,
p_r TINYINT,
p_er TINYINT,
p_bb TINYINT,
p_ibb TINYINT,
p_so TINYINT,
p_hbp TINYINT,
p_wp TINYINT,
p_bk TINYINT,
p_sh TINYINT,
p_sf TINYINT,
p_sb TINYINT,
